                                                                                                                                                                                                                                                                                                                             # Database Configuration
# PostgreSQL database connection string (typically provided by Replit)
DATABASE_URL=xxx

# Server Configuration
# Port for the application (default is 5000)
PORT=5000

# Node Environment
NODE_ENV=development

# Session Management
# Secret key for session encryption - generate a strong random string
SESSION_SECRET=your-very-strong-session-secret-key-here-make-it-long-and-random

# Application Configuration
# Domains where the app will be accessible (comma-separated)
APP_DOMAINS=localhost:5000

# Object Storage Configuration
# Public object search paths (comma-separated bucket paths)
# Format: /bucket-name/path,/another-bucket/path
PUBLIC_OBJECT_SEARCH_PATHS=/dev-public-bucket/public,/dev-bucket/images
defaultBucketID = xxx

# Private object directory for secure file storage
# Format: /bucket-name/private-path
PRIVATE_OBJECT_DIR=/dev-private-bucket/private

# Replit Sidecar Endpoint (used for object storage in production)
# In development, this will be mocked automatically
REPLIT_SIDECAR_ENDPOINT=http://127.0.0.1:1106

GOOGLE_CLIENT_ID=xxx
GOOGLE_CLIENT_SECRET=xxx
